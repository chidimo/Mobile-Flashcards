    "build:ios:production": "eas build --profile development --platform ios",
    "submit:ios": "eas submit --platform android --profile production"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
